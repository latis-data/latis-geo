<?xml version="1.0" encoding="UTF-8"?>

<dataset>
  <metadata name="bwtiff"/> 
  <adapter class="latis.reader.tsml.GeoTiffAdapter"
           location="tif/bw.tif"/>
           
  <function>
    <tuple>
      <real>
        <metadata name="lat"/>
      </real>
      <real>
        <metadata name="lon"/>
      </real>
    </tuple>
    <integer>
      <metadata name="color"/>
    </integer>
  </function>

</dataset>

