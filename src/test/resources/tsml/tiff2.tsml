<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="tiff">

    <adapter class="latis.reader.tsml.GeoTiffAdapter"
             location="datasets/bw.tif"/>
             
    <function>
      <tuple>
        <real name="lat"/>
        <real name="lon"/>
      </tuple>
      <integer name="color"/>
    </function>

  </dataset>

</tsml>
